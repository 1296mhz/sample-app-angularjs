  "moduleResolution": "node",
  "exclude": ["config.js", "data/generate.js", "node_modules", "transpiled"],
  "compilerOptions": {
    "allowJs": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "target": "es5",
    "rootDir": "app",
    "outDir": "transpiled/app",
    "declaration": false,
    "sourceMap": true
  }
}
